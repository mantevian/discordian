  "authors": [
    "mantevian"
  ],
  "contact": {},

  "license": "MIT",
  "icon": "assets/discordian/icon.png",

  "environment": "*",
  "entrypoints": {
    "server": [
      "net.fabricmc.mante.discordian.Discordian"
    ]
  },

  "mixins": [
    "discordian.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.0",
    "fabric": "*",
    "minecraft": ">=1.18"
  }
}